{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {
      "cache": false
    },
    "lint:fix": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    }
  }
}
